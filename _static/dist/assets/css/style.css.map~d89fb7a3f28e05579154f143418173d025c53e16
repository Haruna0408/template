portrait($break-point: $breakpoint-xs) {\n  @media screen and (max-width: $break-point) and (orientation: portrait) {\n    @content;\n  }\n}\n// スマホ横\n@mixin mq-sp-landscape($break-point: $breakpoint-xs) {\n  @media screen and (max-width: $break-point) and (orientation: landscape) {\n    @content;\n  }\n}\n\n// 三角\n@mixin arrow($direction:left, $width:8px, $height:8px, $arrCol:#fff) {\n  content: \"\";\n  display: block;\n  width: 0;\n  height: 0;\n  border-style: solid;\n  @if $direction == left {\n    border-width: $height/2 $width;\n    border-color: transparent $arrCol transparent transparent;\n  }\n  @else if $direction == right {\n    border-width: $height/2 $width;\n    border-color: transparent transparent transparent $arrCol;\n  }\n  @else if $direction == top {\n    border-width: $height $width/2;\n    border-color: transparent transparent $arrCol transparent;\n  }\n  @else if $direction == bottom {\n    border-width: $height $width/2;\n    border-color: $arrCol transparent transparent transparent;\n  }\n  @else {\n    border-width: $height $width/2;\n    border-color: $arrCol transparent transparent transparent;\n  }\n}\n\n//コンテンツ幅\n@mixin l-inner {\n  max-width: 1080px;\n  margin-left: auto;\n  margin-right: auto;\n\n  @media screen and (min-width: 768px) and (max-width: 1024px){\n    width: 96%;\n    max-width: 96%;\n    padding: 0 2%;\n  }\n\n  @media screen and (max-width: 767px){\n    width: 100%;\n    max-width: 100%;\n    padding: 0 4%;\n  }\n}\n\n@mixin l-inner__min {\n  max-width: 720px;\n  margin-left: auto;\n  margin-right: auto;\n}\n\n// ===================================================================\n// font-family\n// ===================================================================\n@mixin gothic {\n  font-family: 'Noto Sans JP', 'メイリオ', Meiryo, sans-serif;\n}\n@mixin mincho {\n  font-family: 'Noto Serif JP', YuMincho, 'Yu Mincho', serif;\n}\n\n\n// ===================================================================\n// font-size → @include fz_vw(xx); （xxにはpixel相当の数値を入れる）\n// ===================================================================\n@function get_vw($size, $viewport:375){\n  $rate: 100 / $viewport;\n  @return $rate * $size * 1vw;\n}\n\n@mixin fz_vw($font_size:10){\n  font-size: $font_size * 1px;\n  font-size: get_vw($font_size);\n}\n\n\n// ===================================================================\n// flex, grid\n// ===================================================================\n@mixin flex {\n    display: flex;\n}\n@mixin flexWrap {\n  display: flex;\n  flex-wrap: wrap;\n}\n@mixin flexColumn {\n  flex-direction: column;\n  > * {\n    max-width: 100%;\n    min-height: 0%;\n  }\n}\n\n@mixin grid {\n  display: grid;\n}\n\n// ===================================================================\n// position\n// ===================================================================\n@mixin center {\n    position: absolute;\n    display: block;\n    content: \"\";\n    left: 50%;\n    transform: translate(-50%, -50%);\n}\n@mixin before {\n    position: absolute;\n    display: block;\n    content: \"\";\n    left: 0;\n}\n@mixin after {\n    position: absolute;\n    display: block;\n    content: \"\";\n    right: 0;\n}\n\n\n// ===================================================================\n// animation\n// ===================================================================\n\n//透過\n// @mixin opacity($value: .6) {\n//   @include mq-min($breakpoint-md) {\n//     transition: .3s;\n//     &:hover {\n//       opacity: $value;\n//     }\n//   };\n// }\n\n// 下からフェードイン\n@mixin scrollFedeIn {\n  opacity: 0;\n  transform: translateY(50px);\n  transition: transform 1.2s ease-out .4s, opacity 1.2s ease-out .4s;\n  &.js-show {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n// テキストラインスライド\n@keyframes maskStart {\n  0%   {\n    right: 100%;\n    left: 0%;\n  }\n  48%  {\n    right: 0%;\n    left: 0%;\n  }\n  52%  {\n    right: 0%;\n    left: 0%;\n  }\n  100% {\n    right: 0%;\n   